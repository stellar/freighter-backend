{
  "extends": "@stellar/tsconfig",
  "compilerOptions": {
    "importHelpers": false,
    "target": "ESNEXT",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "commonjs",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "build",
    "node_modules"
  ]
}